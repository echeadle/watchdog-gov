# Congress.gov API key - Get from https://api.data.gov/signup/
CONGRESS_API_KEY=

# OpenFEC API key - Get from https://api.data.gov/signup/ (or use DEMO_KEY for testing)
FEC_API_KEY=DEMO_KEY

# NewsAPI key - Get from https://newsapi.org/register
NEWS_API_KEY=

# Anthropic API key - Get from https://console.anthropic.com/
ANTHROPIC_API_KEY=

# Application secret key for sessions
SECRET_KEY=your-secret-key-here

# Database URL (SQLite by default)
DATABASE_URL=sqlite+aiosqlite:///./watchdog.db
